  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rimraf dist",
    "build:client": "webpack --mode production --config config/webpack.client.js",
    "build:server": "webpack --mode production --config config/webpack.server.js",
    "build": "npm run clean && npm run build:client && npm run build:server",
    "build:dev:client": "webpack --mode development --watch --config config/webpack.client.js",
    "build:dev:server": "webpack --mode development --watch --config config/webpack.server.js",
    "ssr": "npm run clean && concurrently \"npm run build:dev:*\"",
    "start": "npm run clean && webpack-dev-server --env.SSR_DISABLED=true --mode development --progress --config config/webpack.client.js",
    "server": "nodemon dist"
  },
