  },
  "bugs": {
    "url": "https://github.com/hjakk/react-ssr-starter-kit/issues"
  },
  "pre-commit": [
    "lint"
  ],
  "sideEffects": false,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "webpack-dev-server --mode development",
    "start": "npm run clean && concurrently \"npm run watch\" \"npm run server\"",
    "server": "cross-env PORT=8020 nodemon dist",
    "clean": "rimraf dist",
    "watch": "webpack --mode development -w",
    "build:dev": "webpack --mode development",
    "build:prod": "webpack --mode production",
    "build": "npm run clean && npm run build:prod",
    "lint": "eslint src/client --quiet --ext .ts,.tsx,.js,.jsx -c .eslintrc"
  },
