{
  "name": "react-ssr-starter-kit",
  "version": "0.1.0",
  "author": "hjakk",
  "license": "MIT",
  "description": "A React starter with server side rendering",
  "keywords": [
    "webpack",
    "typescript",
    "react",
    "ssr"
  ],
  "main": "dist/index.js",
  "homepage": "https://github.com/hjakk/react-ssr-starter-kit#readme",
